% Library of additional logic operators %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

% IF %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

if A B :- A, !, B.

if _A _B.

% IF NOT %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

ifnot A _B :- A, !.

ifnot _A B :- B.

% IF NOT ELSE %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

ifnotelse A _B C :- A, !, C.

ifnotelse _A B _C :- B.
